<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
		PUBLIC "-//mybatis.org//DTD Config 4.0//EN"
		"http://mybatis.org/dtd/mybatis-4-config.dtd">
<configuration application="data-service">

	<properties resource="" url="">

	</properties>

	<settings>
		<setting name="logImpl" value="SLF4J"/>
	</settings>

	<typeAliases>
		<typeAlias alias="Integer" type="java.lang.Integer" />
		<typeAlias alias="Long" type="java.lang.Long" />
		<typeAlias alias="HashMap" type="java.util.HashMap" />
		<typeAlias alias="LinkedHashMap" type="java.util.LinkedHashMap" />
		<typeAlias alias="ArrayList" type="java.util.ArrayList" />
		<typeAlias alias="LinkedList" type="java.util.LinkedList" />
	</typeAliases>

	<typeHandlers>
		<typeHandler handler="" javaType="" jdbcType=""/>
	</typeHandlers>

	<environments default="dev">
		<environment id="dev">
			<dataSource id="magician" type="HikaricpPool">
				<property name="jdbcUrl" value="jdbc:mysql://localhost:3306/magician"/>
			</dataSource>
			<dataSource id="boss_board" type="HikaricpPool">
				<property name="jdbcUrl" value="jdbc:mysql://localhost:3306/boss_board"/>
			</dataSource>
		</environment>
	</environments>

	<mappers>
		<mapper url="http://api.dev.xinc818.com/magician/LiveAnchorMapper.xml" />
	</mappers>

</configuration>